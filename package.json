{
  "name": "bblm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8888",
    "build": "next build",
    "start": "next start",
    "move_CNAME_up": "mv docs/CNAME ./CNAME",
    "move_CNAME_down": "mv CNAME docs/CNAME",
