    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "^5.0.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.53",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-styled-components": "^1.11.1",
    "typescript": "^4.0.2"
  },
  "resolutions": {
